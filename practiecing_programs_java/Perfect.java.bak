//*print whether given number is perfect number or not a perfect number*//
class Perfect 
{
	public static void main(String[] args) 
	{
		int n=28;
		int sum=0;
		for(int i=1;i<=n;i++)
		{
			if(n%i==0)
			{
		    sum=sum+i;
			}
			if(sum==n)
			{
		    System.out.println("perfect number");
			}
			else
			{
			System.out.println("not a perfect number");
			}
		}
	}
}
	
