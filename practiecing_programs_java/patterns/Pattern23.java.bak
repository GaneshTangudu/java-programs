class Pattern23 
{
	public static void main(String[] args) 
	{
		int n = 1;
		

		for (int i=1;i<=5 ;i++ )
		{
			char ch = 'a';
			for (int j=1;j<=5 ;j++)
			{
				if (i<j)
				{
					System.out.print(ch++ +" ");
				}
				else
				{
					
					System.out.print(n++ +" ");
				}
				if (n>9)
				{
					n = 0;
				}
				
			}
			System.out.println();
		}
	}
}
